{
  "name": "hannah-blog",
  "version": "1.0.0",
  "description": "Hannah's Blog",
  "main": "app.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "concat --use autoprefixer -b \"last 10 version\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "start": "node app.js"
  },
  "author": "TG",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concat": "^1.0.3",
    "ejs": "^3.1.6",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/THG20203/Hannah-Blog-SASS.git"
  },
  "bugs": {
    "url": "https://github.com/THG20203/Hannah-Blog-SASS/issues"
  },
  "homepage": "https://github.com/THG20203/Hannah-Blog-SASS#readme"
}
